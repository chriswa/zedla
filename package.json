{
  "name": "zedla",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm && npx -y check-node-version --node \">=22\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ci": "vitest --run --coverage"
  },
  "devDependencies": {
    "@chriswa/ts-config": "^0.0.6",
    "@swc/core": "^1.13.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "unplugin-swc": "^1.5.6",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "keycode-js": "^3.1.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  }
}
